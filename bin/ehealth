#!/usr/bin/env ruby

require "bundler/setup"
require "ehealth"

opt = ARGV[0]

if opt == '-i'
  ARGV.clear
  Ehealth::Cli.new
elsif opt == '-b'
  file = ARGV[1]
  puts 'Please pass a file' unless file
else
  puts 'Okay?'
end

